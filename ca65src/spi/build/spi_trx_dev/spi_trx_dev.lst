000915r 1               ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
000915r 1               ;;
000915r 1               ;;    interrupt is triggered by HIGH edge on VIA CA1 pin
000915r 1               ;;     PORTA low nibble (keypad columns) inputs are diode ORed to CA1
000915r 1               ;;
000915r 1               ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
000915r 1               
000915r 1               irq:
000915r 1               
000915r 1               ; put registers on the stack while handling the IRQ
000915r 1               
000915r 1  48             pha
000916r 1  DA             phx
000917r 1  5A             phy
000918r 1               
000918r 1               ;  find responsible hardware
000918r 1               
000918r 1               ;  Is it VIA_1?
000918r 1               
000918r 1  AD 0D 60       lda IFR_1       ; if IFR_1 has Bit7 set (ie sign=NEGATIVE) then it IS the source of the interrupt
00091Br 1  10 25          bpl next_device ; if it's not set (ie sign=POSITIVE) then branch to test the next possible device
00091Dr 1               
00091Dr 1               ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
00091Dr 1               ;;
00091Dr 1               ;; IFR Flags
00091Dr 1               ;; B7  B6  B5  B4  B3  B2  B1  B0
00091Dr 1               ;; IRQ TI1 TI2 CB1 CB2 SR CA1 CA2
00091Dr 1               ;;
00091Dr 1               ;; Interrupt source is found by sequentially shifting IFR bit left to put bit-of-interest into the CARRY place
00091Dr 1               ;; and then branching based on whether CARRY is SET or not
00091Dr 1               ;;
00091Dr 1               ;; Only add tests for IRQ sources in use, and adjust the ASLs in each test as necessary
00091Dr 1               ;;
00091Dr 1               ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
00091Dr 1               
00091Dr 1               test_cb1:
00091Dr 1  0A             asl
00091Er 1  0A             asl
00091Fr 1  0A             asl
000920r 1  0A             asl
000921r 1  90 09          bcc test_cb2
000923r 1  2C 00 60       bit PORTB_1
000926r 1  20 rr rr       jsr cb1_handler
000929r 1  4C rr rr       jmp exit_irq
00092Cr 1               
00092Cr 1               test_cb2:
00092Cr 1  0A             asl
00092Dr 1  90 09          bcc test_ca1
00092Fr 1  2C 00 60       bit PORTB_1
000932r 1  20 rr rr       jsr cb2_handler
000935r 1  4C rr rr       jmp exit_irq
000938r 1               
000938r 1               test_ca1:
000938r 1               
000938r 1  0A             asl           ; shift CA1 bit into the CARRY bit & test
000939r 1  0A             asl
00093Ar 1  90 06          bcc exit_irq        ; carry clear = leave
00093Cr 1  20 rr rr       jsr keypad_handler  ; carry not clear = handle the CA1 interrupt (keypad)
00093Fr 1  4C rr rr       jmp exit_irq
